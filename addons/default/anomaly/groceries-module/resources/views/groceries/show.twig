{% extends "theme::layouts/default" %}

{% block content %}
<div class="row">
    <div class="col-md-4">
        <div class="row">
            <div class="col-md-12"><strong>NAME:</strong> {{ grocery.name }}</div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="row">
            <div class="col-md-12"><strong>SLUG:</strong> {{ grocery.slug }}</div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="row">
            <div class="col-md-12"><strong>DESCRIPTION:</strong> {{ grocery.description }}</div>
        </div>
    </div>
</div>
{% endblock %}
